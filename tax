import sys
import math


lines = sys.stdin.readlines() 
ans = []

for line in lines:
    line = line.strip() 
    if not line:
        sys.exit(1)
    try:
        x = float(line)
    except ValueError:    
        sys.exit(1)

for line in lines:
    x = float(line.strip())
    ans.append(str(math.floor(x * 1.10)))

print("\n".join(ans))
