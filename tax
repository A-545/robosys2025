import sys
import math

lines = sys.stdin.readlines()
ans = []

for line in lines:
    original = line.rstrip("\n")
    line = original.strip()

    if not line:
        print(f"無効な入力: {original}", file=sys.stderr)
        sys.exit(1)
    try:
        x = float(line)
        if x < 0:
            print(f"無効な入力: {original}", file=sys.stderr)
            sys.exit(1)
        ans.append(str(math.floor(x * 1.10)))
    except ValueError:
        print(f"無効な入力: {original}", file=sys.stderr)
        sys.exit(1)

for a in ans:
    print(a)
